<p id="notice"><%= notice %></p>

<h1>Listing Portfilos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @portfilos.each do |portfilo| %>
      <tr>
        <td><%= portfilo.name %></td>
        <td><%= portfilo.info %></td>
        <td><%= link_to 'Show', portfilo %></td>
        <td><%= link_to 'Edit', edit_portfilo_path(portfilo) %></td>
        <td><%= link_to 'Destroy', portfilo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Portfilo', new_portfilo_path %>
